<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Authors>Vadim Kandrushin</Authors>
    <Description>super simple client to Zadarma IP calls</Description>
    <RepositoryUrl>https://github.com/ihtfw/nZadarma</RepositoryUrl>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>nZadarma</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>nZadarma</Product>
    <RepositoryType>git</RepositoryType>
    <PackageTags>zadarma</PackageTags>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>2008.21.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>
</Project>
